<?xml version="1.0" encoding="UTF-8"?>
<rule>
	<tableRule name="rule1" forceHit="true"
		unless="SELECT,INSERT,UPDATE,DELETE,CALL">
		<columns>INQUIRY_ID</columns>
		<dbRuleList>
			<dbRule><![CDATA[
                       #set($sub_str="0")
                       $!substr
                    ]]>
			</dbRule>
		</dbRuleList>
		<tbRuleList>
			<tbRule><![CDATA[
                       #set($sub_str=$INQUIRY_ID%4+"")
                       #set($prefix="_0"+$sub_str)
                       $!prefix
                    ]]>
			</tbRule>
		</tbRuleList>
		<tbPrefix>
				<![CDATA[ 
                    def map = [:];
                    def list = [];
                    for(int i=0; i<3; i++){
                      list.add("_01");
                         map.put(0,list);
                    }
                  
                    return map; 
                ]]>
		</tbPrefix>
	</tableRule>
	<tableRule name="rule2" forceHit="true"
		unless="SELECT,INSERT,UPDATE,DELETE,CALL">
		<columns>ORDER_ID</columns>
		<dbRuleList>
			<dbRule><![CDATA[
                      #set($t=$numberUtil.toInt($ORDER_ID) % 4)##
                      $!t
                    ]]>
			</dbRule>
		</dbRuleList>
		<tbRuleList>
			<tbRule><![CDATA[
                       #set($sub_str="")##
                       $!sub_str
                    ]]>
			</tbRule>
		</tbRuleList>
		<tbPrefix>
				<![CDATA[ 
                    def map = [:];
                    def list = [];
                    for(int i=0; i<4; i++){
                      list=[]
                      list.add("");
                         map.put(i,list);
                    }
                  
                    return map; 
                ]]>
		</tbPrefix>
	</tableRule>

</rule>